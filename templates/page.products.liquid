  <script>
    // Function to update the active map and address
    function updateActiveMap(index) {
      // Remove 'active' class from all maps
      document.querySelectorAll('.map').forEach(map => {
        map.classList.remove('active');
      });
      
      // Remove 'active' class from all address items
      document.querySelectorAll('.products-locations__item').forEach(item => {
        item.classList.remove('active');
      });
      
      // Add 'active' class to the selected map
      document.querySelector('.map[data-index="' + index + '"]').classList.add('active');
      
      // Add 'active' class to the selected address item
      document.querySelector('.products-locations__item[data-index="' + index + '"]').classList.add('active');
    }

    // Set the first map and address as active by default
    updateActiveMap(0);

    // Add click event listener to all address items
    document.querySelectorAll('.products-locations__item').forEach(address => {
      address.addEventListener('click', function() {
        let index = this.getAttribute('data-index');
        updateActiveMap(index);
      });
    });
  </script>
